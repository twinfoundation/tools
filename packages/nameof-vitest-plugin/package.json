{
	"name": "@twin.org/nameof-vitest-plugin",
	"version": "0.0.1-next.15",
	"description": "A Vitest plugin which perform the nameof transformation",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/twinfoundation/tools.git",
		"directory": "packages/nameof-vitest-plugin"
	},
	"author": "martyn.janes@iota.org",
	"license": "Apache-2.0",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "tsc",
		"build:watch": "tsc-watch --onSuccess \"npm run bundle:esm\"",
		"test": "vitest --run --config ./vitest.config.ts --no-cache",
		"coverage": "vitest --run --coverage --config ./vitest.config.ts --no-cache",
		"bundle:esm": "rollup --config rollup.config.mjs --environment MODULE:esm",
		"bundle:cjs": "rollup --config rollup.config.mjs --environment MODULE:cjs",
		"bundle": "npm run bundle:esm && npm run bundle:cjs",
		"docs:clean": "rimraf docs/reference",
		"docs:generate": "typedoc",
		"docs": "npm run docs:clean && npm run docs:generate",
		"dist": "npm run clean && npm run build && npm run test && npm run bundle && npm run docs"
	},
	"dependencies": {
		"@twin.org/nameof-transformer": "0.0.1-next.15"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "2.1.8",
		"copyfiles": "2.4.1",
		"rimraf": "6.0.1",
		"rollup": "4.30.0",
		"tsc-watch": "6.2.1",
		"typedoc": "0.27.6",
		"typedoc-plugin-markdown": "4.4.1",
		"typescript": "5.7.2",
		"vitest": "2.1.8"
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.mjs",
	"exports": {
		".": {
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/esm/index.mjs",
			"types": "./dist/types/index.d.ts"
		}
	},
	"files": [
		"dist/cjs",
		"dist/esm",
		"dist/types",
		"locales",
		"docs"
	]
}
